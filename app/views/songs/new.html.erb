<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>

  <%= f.collection_select :genre_id, Genre.all, :id, :name %>

  <%= f.label :notes_1 %>
  <input name="song[notes][]" id="song_notes_1">
  <%= f.label :notes_2 %>
  <input name="song[notes][]" id="song_notes_2">
  <%= f.label :notes_3 %>
  <input name="song[notes][]" id="song_notes_3">

  <%= f.submit %>
<% end %>

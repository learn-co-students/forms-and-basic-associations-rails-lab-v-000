<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>

  <%= f.label :note_contents %>
  <%= f.text_field :note_contents, :id => 'song_notes_1' %>


  <%= f.collection_select :song, :genre, Genre.all, :name %>

  <%= f.submit %>
<% end %>

<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_id %>
  <%= f.collection_select :genre_name, Genre.all, :name, :name %>
  <%= f.label :note_content %>
    <input id="song_notes_1" name="song[note_contents][]">
    <input id="song_notes_2" name="song[note_contents][]">
  <br />

  <%= f.submit %>
<% end %>

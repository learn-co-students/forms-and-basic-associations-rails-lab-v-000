<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>

  <%= f.collection_select :genre_id, Genre.all, :id, :name, prompt: true %>

  <%= f.label :notes %>
    <input type="text" name="song[note_contents][]" id="song_notes_1">
    <input type="text" name="song[note_contents][]" id="song_notes_2">
    <br>


  <br />

  <%= f.submit %>
<% end %>

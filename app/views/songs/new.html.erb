<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>:
    <%= f.text_field :title %><br>
  <%= f.label "Artist Name: " %>
    <%= f.text_field :artist_name %><br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  <input name="song[note_contents][]" id="note_contents_1">
  <input name="song[note_contents][]" id="note_contents_2">
  <input name="song[note_contents][]" id="note_contents_3">
  <%= f.submit %>
<% end %>



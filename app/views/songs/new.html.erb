<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />

  <%= f.collection_select :genre_id, Genre.all, :id, :name %>

  <%= f.label :song_notes_1 %>
  <%= f.text_field :song_notes_1 %>
  <br />

  <%= f.label :song_notes_2 %>
  <%= f.text_field :song_notes_2 %>
  <br />


  <%= f.submit %>
<% end %>

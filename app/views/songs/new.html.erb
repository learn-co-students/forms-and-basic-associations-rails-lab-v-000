<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :song_title %>
  <%= f.text_field :title %>
  <br>

  <%= f.label :song_artist_name %>
  <%= f.text_field :artist_name %>
  <br>

  <%= f.label :song_genre_id %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br>

  <input type="text" id="song_notes_1" name="song[note_contents][]"><br>
  <input type="text" id="song_notes_2" name="song[note_contents][]"><br>

	

  <%= f.submit "commit" %>
<% end %>



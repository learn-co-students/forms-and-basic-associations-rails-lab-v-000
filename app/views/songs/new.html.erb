<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <p>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br /><br />
  <%= f.label :genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br /><br />
  Song Notes: <input name='song[note_contents][]', id= "song_notes_1"> <br />
  Song Notes: <input name="song[note_contents][]", id= "song_notes_2"> <br />
  Song Notes: <input name="song[note_contents][]", id="song_notes_3"> <br />
  <br />
  <%= f.submit %>
<% end %>

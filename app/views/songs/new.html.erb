<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />

  <%= f.select :genre_id, Genre.all.collect {|p| [ p.name, p.id ] } %>
  <br />

  <%= f.label :song_notes_1 %>
  <%= f.text_field :note_contents, multiple: true, id: 'song_notes_1'%>
  <%= f.label :song_notes_2 %>
  <%= f.text_field :note_contents, multiple: true, id: 'song_notes_2'%>

  <%= f.submit %>
<% end %>

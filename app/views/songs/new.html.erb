<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>

  <%= f.collection_select :genre, Genre.all, :id, :name %>
  <%= f.text_field :genre %>

  <input name="song[song_notes_1][]">
  <input name="song[song_notes_2][]">
  <input name="song[song_notes_3][]">



  <%= f.submit %>
<% end %>

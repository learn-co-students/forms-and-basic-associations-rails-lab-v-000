<h1>Create a Song</h1>

<%= form_for @song do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name%>
  <%= f.text_field :artist_name %>
  <%= f.label :genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <%= f.label :notes_1 %>
  <%= f.text_area :notes_1 %>
  <%= f.label :notes_2 %>
  <%= f.text_area :notes_2 %>
  <br />

  <%= f.submit %>
<% end %>

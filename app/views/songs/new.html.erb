<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  
  
    <%=f.label :song_artist_name%>
  <%= f.text_field :artist_name, id: "song_artist_name"%>
  
  
  <%= f.label :song_genre_id %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  
  
  <%=f.label :song_note_1%>
  <input name="song[note_contents][]" id = "song_notes_1">
  
  
  <%=f.label :song_note_2%>
  <input name="song[note_contents][]" id = "song_notes_2">
  

  <%= f.submit %>
<% end %>



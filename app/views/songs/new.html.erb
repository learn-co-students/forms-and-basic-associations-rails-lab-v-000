<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist %>
  <%= f.text_field :artist_name %>
  <p><%= f.collection_select :genre_id, Genre.all, :id, :name %></p>
  <p>Notes</p>
  <input type="text" name="song[note_contents][]" id="song_notes_1" >
  <input type="text" name="song[note_contents][]" id="song_notes_2" >
  <br />
  <%= f.submit %>
<% end %>

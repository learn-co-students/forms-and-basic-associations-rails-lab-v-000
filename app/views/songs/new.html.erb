<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.collection_select :genre_name, Genre.all, :name, :name, {}, id: "song_genre_id" %>
  <%= text_field_tag 'song[note_contents][]', value = nil, id: 'song_notes_1' %>
  <%= text_field_tag 'song[note_contents][]', value = nil, id: 'song_notes_2' %>
  <br />

  <%= f.submit 'commit'%>
<% end %>

<h1>Create a Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />

  <%= f.label :genre_id %>
  <%= f.select :genre_id, options_for_select(Genre.all.collect{ |u| [u.name, u.id] }) %>
  <br />

  <input id="song_notes_1" name="song[notes][]">
  <input id="song_notes_2" name="song[notes][]">

  <%= f.submit %>
<% end %>



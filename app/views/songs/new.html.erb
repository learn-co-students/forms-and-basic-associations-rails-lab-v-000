<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <%= f.text_field :artist_name %>

  <%= f.text_field :note_contents, name: "song[note_contents][]", id: "song_notes_1"  %>
  <%= f.text_field :note_contents, name: "song[note_contents][]", id: "song_notes_2" %>
  

  <%= f.submit %>
<% end %>



<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.label :genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br />
  <%= f.label :notes %>
  <input name="song_notes_1",>
  <input name="song_notes_2">
  <input name="song_notes_3">
  <br />

  <%= f.submit %>
<% end %>

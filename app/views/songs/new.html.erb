<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <input type="textbox" name="song[note_contents][]" id="song_notes_1"><br></br>
  <input type="textbox" name="song[note_contents][]" id="song_notes_2"><br></br>
  <br />

  <%= f.submit "commit" %>
<% end %>

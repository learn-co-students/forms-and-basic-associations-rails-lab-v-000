<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name, :artist %><%= f.text_field :artist_name %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <input name="song[note_content][]" id="song_notes_attributes_0_content">
  <input name="song[note_content][]" id="song_notes_attributes_1_content">
  <br />

  <%= f.submit %>
<% end %>

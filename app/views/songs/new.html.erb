<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, id: "song_title" %><br />

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %><br />

  <%= f.collection_check_boxes :genre, Genre.all, :id, :name %><br /><br />

  
  <br />

  <%= f.submit :commit %>
<% end %>

<%= form_for @song do |f| %>
  <%= f.label :title %><br />
  <%= f.text_field :title %><br />

  <br />

  <%= f.label :artist_name %><br />
  <%= f.text_field :artist_name %><br />

  <br />

  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br />

  <br />

  <p>Notes</p>
  <input id="song_notes_1" name="song[note_contents][]"><br />
  <input id="song_notes_2" name="song[note_contents][]"><br />
  <input id="song_notes_3" name="song[note_contents][]"><br />

  <br />

  <%= f.submit %>
<% end %>

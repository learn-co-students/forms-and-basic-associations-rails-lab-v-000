<h1>Create a Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= select "song", "genre_id", Genre.all.collect {|p| [ p.name, p.name ] } %>
  <br />
  <input id="song_notes_1" name="song[notes][]">
  <br />
  <input id="song_notes_2" name="song[notes][]">
  <br />
  <input id="song_notes_3" name="song[notes][]">
  <br />
  <%= f.submit %>

<% end %>



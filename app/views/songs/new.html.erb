<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br></br>
  <%= f.label :artist %>
  <%= f.text_field :artist_name%>
  <br></br>
  <%=f.collection_select :genre_id, Genre.all, :id, :name%>
  <br></br>
  <%=f.label :notes%>
  <%=f.text_area :notes_1%>
  <br></br>
  <%=f.label :notes%>
  <%=f.text_area :notes_2%>
  <br></br>
  <%= f.submit %>
<% end %>

<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %><br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
    <%= f.label :Note1 %>
    <input name="song[notes_ids][]"><br>
    <%= f.label :Note2 %>
    <input name="song[notes_ids][]"><br>
    <%= f.label :Note3 %>
    <input name="song[notes_ids][]"><br>
  
  <%= f.submit %>
<% end %>







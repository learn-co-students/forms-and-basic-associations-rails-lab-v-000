<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br /><br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br /><br />
  <%= f.label :genre_name %>
  <%= f.collection_select :genre_id , Genre.order(:name), :id, :name, prompt: true %>
  <input name="song[song_notes][]" id="song_notes_1" type="text">
  <input name="song[song_notes][]" id="song_notes_2" type="text">

  <%= f.submit %>
<% end %>

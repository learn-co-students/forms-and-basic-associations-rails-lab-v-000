<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :song_title %>
  <%= f.text_field :title %>
  <%= f.label :artist %>
  <%= f.text_field :artist_name, id: :song_artist_name %>
  <%= f.collection_select :genre_name, Genre.all, :name, :name, {}, {:id => 'song_genre_id'}%>
  <%= f.label :note_contents%>
  <input id="song_notes_1" name="song[note_contents][]">
  <input id="song_notes_2" name="song[note_contents][]">
  <input id="song_notes_3" name="song[note_contents][]">
  <%= f.submit %>
<% end %>

<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>

  <%= f.label :song_notes_1 %>
  <%= f.text_field :song_notes_1 %>

  <%= f.collection_select :genre, Genre.all, :id, :name %>

  <input name="song[note_ids][]">
  <input name="song[note_ids][]">
  <input name="song[note_ids][]">

  <br />

  <%= f.submit %>
<% end %>

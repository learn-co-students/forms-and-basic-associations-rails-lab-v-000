<h1>Create a Song</h1>

<%= form_for @song do |f| %>
<%= f.label :title %>
<%= f.text_field :title %>
<br />
<%= f.label :Artist %>
<%= f.text_field :artist_name=%>
<br>
<%= f.label :Genre %>
<%= collection_select(:song, :genre_id, Genre.all, :id, :name)%>
<br>
<%= f.label :note_1 %>
<input name='song[note_contents][]' id='song_notes_1'> <br>
<%= f.label :note_2 %>
<input name='song[note_contents][]' id='song_notes_2'><br>
<%= f.label :note_3 %>
<input name='song[note_contents][]' id='song_notes_3'>
<br>
<%= f.submit%>
<% end %>


<%= form_for @song do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :artist_name %>
    <%= f.text_field :artist_name %>

    <%= f.collection_select :genre_id, Genre.all, :id, :name %>
    <label for="song_notes_1">Note 1</label>
    <input id="song_notes_1" name="song[note_contents][]">
    <label for="song_notes_2">Note 2</label>
    <input id="song_notes_2" name="song[note_contents][]">
    <label for="song_notes_3">Note 3</label>
    <input id="song_notes_3" name="song[note_contents][]">
    <%= f.submit %>
<% end %>

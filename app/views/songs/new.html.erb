<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br/>
  <%= f.label :artist %>
  <%= f.text_field :artist_name %>
  <br/>
  <%= f.label :genre %>
  <%= f.collection_select :genre, Genre.all, :id, :name %>
  <br/>
  <%= f.label :notes %>
  <%= f.text_area :notes %>
  <%=  %>
  <br/>
  <%= f.submit %>
<% end %>

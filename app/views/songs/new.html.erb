<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_id, :artist %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.label :genre_id %>
  <%= f.collection_select(:genre_id, Genre.all, :id, :name )%>

  <input type="text" name="song[note_contents][]" value="" id="song_notes_1">
  <input type="text" name="song[note_contents][]" value="" id="song_notes_2">




  <%= f.submit %>
<% end %>

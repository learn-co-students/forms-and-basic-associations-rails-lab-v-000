<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name  %>
  <br>
  <%= f.label :genre_name %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name  %>
  <br>
  <%= f.label :notes %>
  <input name="song[song_notes][]">
  <input name="song[song_notes][]">
  <input name="song[song_notes][]">
  <br><br>
  <%= f.submit %>
<% end %>

<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.collection_select :genre_name, Genre.all, :name, :name %>
  <label for="song_notes_1">Song Notes 1</label>
  <input type="text" name="song[note_contents][]" id="song_notes_1">
  <label for="song_notes_2">Song Notes 2</label>
  <input type="text" name="song[note_contents][]" id="song_notes_2">
  <label for="song_notes_2">Song Notes 3</label>
  <input type="text" name="song[note_contents][]" id="song_notes_3">

  <br>
  <%= f.submit %>
<% end %>

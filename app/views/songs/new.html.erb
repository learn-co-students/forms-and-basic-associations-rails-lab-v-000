<h1>Create a Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <br>
  <%= f.label :artist_name %><br>
  <%= f.text_field :artist_name %><br>
  <br>
  <%= f.label :genre_id %><br>
  <%= f.collection_select :genre_id, Genre.all, :name, :name %><br>
  <br>


  <%= f.submit %>

<% end %>

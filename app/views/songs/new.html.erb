<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :artist_name %>:
  <%= f.text_field :artist_name %><br>

  <%= f.label :title %>:
  <%= f.text_field :title %> <br>

  Genre: <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>

  Notes: <input name="song[note_contents][]" id="song_notes_1"><br>
  Notes: <input name="song[note_contents][]" id="song_notes_2"><br>
  Notes: <input name="song[note_contents][]" id="song_notes_3"><br>

  <%= f.submit %>
<% end %>

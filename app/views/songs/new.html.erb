<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <%= f.label :note %>
    <li><input name="song[note_contents][]" id="song_notes_1" type="text"></li>
    <li><input name="song[note_contents][]" id="song_notes_2" type="text"></li>
    <li><input name="song[note_contents][]" id="song_notes_3" type="text"></li>
    <br>
  <%= f.submit %>
<% end %>

<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />

  <%= f.collection_select :genre_id, Genre.all, :id, :name %>  
  <br />

  <label for="song_notes_1">Notes 1</label>
  <input type="text" name="song[note_contents][]" id="song_notes_1">
  <label for="song_notes_2">Notes 2</label>
  <input type="text" name="song[note_contents][]" id="song_notes_2">
  <label for="song_notes_3">Notes 3</label>
  <input type="text" name="song[note_contents][]" id="song_notes_3">

  <%= f.submit %>
<% end %>
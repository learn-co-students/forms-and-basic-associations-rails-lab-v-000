<h1>Create a Song</h1>


<%= form_for @song do |f| %>
  <div>
    <div><%= f.label :title %></div>
    <div><%= f.text_field :title %></div>
  </div>

  <div>
    <div><%= f.label :artist_name %></div>
    <div><%= f.text_field :artist_name %></div>
  </div>
  
  <div><%= f.label :genre %></div>
  <div>
    <%= f.collection_select :genre_id, Genre.all, :id, :name %>
    
    <input name='song[note_contents][]' id='song_notes_1'>
    <input name='song[note_contents][]' id='song_notes_2'>
  </div>
    
  <%= f.submit %>
<% end %>


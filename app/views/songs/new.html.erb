<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br/>
  <%= f.label :artist %>
  <%= f.text_field :artist_name %>
  <br/>
  <%= f.label :genre_id, :genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br/>
  <%= f.label :notes_1, :Notes %>
  <%= f.text_field  :notes_1,  name:"song[note_contents][]" ,value: "" %>

  <br/>
  <%= f.label :notes_2, :Notes %>
  <%= f.text_field  :notes_2,  name:"song[note_contents][]" ,value: "" %>

  <br/>
  <%= f.submit %>
<% end %>

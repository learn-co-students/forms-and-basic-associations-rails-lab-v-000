<%-# THIS FILE DOES NOT NEED TO BE CHANGED TO PASS THE LAB.  -%>

<h1>Editing <%= @song.title %></h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br>
  <%= f.label :genre_name %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  <h3>NOTES</h3>
  <input name='song[note_contents][]' id='song_notes_1'>
  <input name='song[note_contents][]' id='song_notes_2'>
  <%= f.submit %>
<% end %>

<h1>Editing <%= @song.title %></h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.label :genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br>
  <%= f.fields_for :notes do |note| %>
    <%= note.label :note %>
    <%= note.text_field :content %><br>
    <br>
  <% end %>

  <br>
  <%= f.submit %>
<% end %>
